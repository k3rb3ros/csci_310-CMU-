<Window x:Class="POS_Gui.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="search_bar" HorizontalAlignment="Left" Height="31" Margin="10,10,0,0" TextWrapping="Wrap" Text="Search" VerticalAlignment="Top" Width="497" LostFocus="search_bar_LostFocus" KeyDown="search_bar_KeyDown"/>
        <WebBrowser x:Name="search_results" HorizontalAlignment="Left" Height="201" Margin="10,46,0,0" VerticalAlignment="Top" Width="497"/>
        <Button x:Name="add_cart_btn" Content="Add To Cart" HorizontalAlignment="Left" Margin="10,252,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="checkout_btn" Content="Checkout" HorizontalAlignment="Left" Margin="432,252,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
